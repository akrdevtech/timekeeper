{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:all": "electron-builder build --mac --win --linux -c.extraMetadata.main=tk_app/main.js --publish never",
    "package:lin": "electron-builder build --linux -c.extraMetadata.main=tk_app/main.js --publish never",
    "package:win": "electron-builder build --win --publish never",
    "package:mac": "electron-builder build --mac -c.extraMetadata.main=tk_app/main.js --publish never"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^20.1.4",
    "electron-builder": "^23.3.3"
  },
  "dependencies": {},
  "build": {
    "appId": "timekeeper",
    "files": [
      "**/node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "timekeeper",
      "owner": "akhilkrishnanr"
    }
  }
}