{
  "name": "timekeeper",
  "version": "0.1.0",
  "private": true,
  "productName": "Timekeeper",
  "main": "electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@mui/x-date-pickers": "^5.0.0-beta.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "joi": "^17.6.0",
    "morgan": "~1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "sequelize": "^6.21.4",
    "sqlite3": "^5.0.11",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && electron .",
    "make:src": "mkdir build\\src",
    "build:lin": "npm run make:src && cp -r electron/ build/electron && cp -r src/shared/ build/src/shared",
    "build:win": "npm run make:src && robocopy electron build/electron /S & robocopy src/shared build/src/shared /S",
    "package:all": "electron-builder build --mac --win --linux -c.extraMetadata.main=build/electron/main.js --publish never",
    "package:lin": "electron-builder build --linux -c.extraMetadata.main=build/electron/main.js --publish never",
    "package:win": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "package:mac": "electron-builder build --mac -c.extraMetadata.main=build/electron/main.js --publish never",
    "release:lin": "yarn run build && yarn run build:lin && yarn run package:lin",
    "release:win": "yarn run build && yarn run build:lin && yarn run package:win",
    "dev:old": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && SET ELECTRON_START_URL=http://localhost:3000 && electron .\"",
    "dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && SET ELECTRON_START_URL=http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^20.1.4",
    "electron-builder": "^23.3.3",
    "wait-on": "^5.2.1"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "timekeeper",
      "owner": "akhilkrishnanr"
    }
  }
}
